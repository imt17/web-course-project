\\u043E\\u0432\\u043D\\u0430 \\u0447\\u0430\\u0441\\u0442\\u0438\\u043D\\u0430 \\u043C\\u0456\\u0441\\u0442\\u0430 \\u2014 \\u041C\\u0430\\u043D\\u0433\\u0435\\u0442\\u0442\\u0435\\u043D; \\u043C\\u0443\\u0437\\u0435\\u0457, \\u043C\\u0438\\u0441\\u0442\\u0435\\u0446\\u044C\\u043A\\u0456 \\u0433\\u0430\\u043B\\u0435\\u0440\\u0435\\u0457; \\u043F\\u0430\\u043C'\\u044F\\u0442\\u043A\\u0438 \\u0430\\u0440\\u0445\\u0456\\u0442\\u0435\\u043A\\u0442\\u0443\\u0440\\u0438 XVII \\u0442\\u0430 XIX \\u0441\\u0442\\u043E\\u043B\\u0456\\u0442\\u044C; \\u0447\\u0438\\u0441\\u043B\\u0435\\u043D\\u043D\\u0456 \\u0445\\u043C\\u0430\\u0440\\u043E\\u0447\\u043E\\u0441\\u0438, \\u043F\\u0435\\u0440\\u0435\\u0432\\u0430\\u0436\\u043D\\u043E \\u043D\\u0430 \\u041C\\u0430\\u043D\\u0433\\u0435\\u0442\\u0442\\u0435\\u043D\\u0456; \\u0431\\u0430\\u0433\\u0430\\u0442\\u043E \\u043F\\u0430\\u043C'\\u044F\\u0442\\u043D\\u0438\\u043A\\u0456\\u0432 (\\u0437\\u043E\\u043A\\u0440\\u0435\\u043C\\u0430 \\u0422. \\u0428\\u0435\\u0432\\u0447\\u0435\\u043D\\u043A\\u043E\\u0432\\u0456 \\u0432 \\u0410\\u0440\\u0440\\u043E\\u0432-\\u043F\\u0430\\u0440\\u043A\\u0443); \\u0421\\u0442\\u0430\\u0442\\u0443\\u044F \\u0421\\u0432\\u043E\\u0431\\u043E\\u0434\\u0438. \\u0423 \\u041D\\u044C\\u044E-\\u0419\\u043E\\u0440\\u043A\\u0443 \\u0440\\u043E\\u0437\\u0442\\u0430\\u0448\\u043E\\u0432\\u0430\\u043D\\u043E \\u0448\\u0442\\u0430\\u0431-\\u043A\\u0432\\u0430\\u0440\\u0442\\u0438\\u0440\\u0443 \\u041E\\u041E\\u041D. \\u0417\\u0430 \\u0440\\u0435\\u0437\\u0443\\u043B\\u044C\\u0442\\u0430\\u0442\\u0430\\u043C\\u0438 \\u043E\\u043F\\u0438\\u0442\\u0443\\u0432\\u0430\\u043D\\u043D\\u044F, \\u0449\\u043E \\u043F\\u0440\\u043E\\u0432\\u0456\\u0432 \\u0436\\u0443\\u0440\\u043D\\u0430\\u043B Forbes \\u0441\\u0435\\u0440\\u0435\\u0434 \\u0444\\u0430\\u0445\\u0456\\u0432\\u0446\\u0456\\u0432 2010 \\u0440\\u043E\\u043A\\u0443, \\u041D\\u044C\\u044E-\\u0419\\u043E\\u0440\\u043A \\u043F\\u043E\\u0442\\u0440\\u0430\\u043F\\u0438